{
    "name": "andrey-helldar/xml-core",
    "description": "The XML Core package.",
    "type": "library",
    "license": "MIT",
    "keywords": ["xml", "core"],
    "support": {
        "issues": "https://github.com/andrey-helldar/xml-core/issues",
        "source": "https://github.com/andrey-helldar/xml-core"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-xml": "*"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Core\\Xml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
