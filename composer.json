{
    "name": "dragon-code/xml-core",
    "description": "Preset settings for packages that work with XML files.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "xml",
        "core",
        "dragon-code",
        "dragon",
        "andrey-helldar"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/xml-core/issues",
        "source": "https://github.com/TheDragonCode/xml-core"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-dom": "*",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/validation": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "symfony/http-kernel": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0 || ^6.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\Core\\Xml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
